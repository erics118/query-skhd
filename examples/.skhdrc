cmd - n : echo "hello everyone"

ctrl + rshift + ralt + cmd - 4 : echo "four modifiers"

meh - m : echo "meh"

hyper - h : echo "hyper"

rshift - p : echo "passthrough"

:: test_mode @ : echo "entered test_mode"

test_mode < a : echo "a" # prints an 'a'

test_mode < escape ; default

alt - t ; test_mode
